{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {"arrow-parens": ["error", "as-needed", { "requireForBlockBody": true }],
      "max-len": ["error", 150],
      "no-unused-vars": ["off"],
      "import/no-unresolved": ["off"],
      "import/extensions": ["off"],
      "no-restricted-syntax": ["off"],
      "no-loop-func": ["off"]
    }
}
